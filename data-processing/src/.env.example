# AWS Credentials (if not using IAM roles)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_DEFAULT_REGION=us-east-1

# OpenSearch Credentials
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=your_password_here

# S3 Configuration
S3_BUCKET_NAME=your-bucket-name

# Optional: Feature Flags
# LLM_FALLBACK_ENABLED=true
# RELATED_TAGS_ENABLED=true
# SIMILARITY_THRESHOLD=0.3

# Optional: SSH Tunnel Configuration (for local development with jumphost)
JUMPHOST=jumphost-sg.castlery.com
JUMPHOST_USER=ec2-user
SSH_KEY_PATH=~/.ssh/id_rsa

# Optional: RDS Configuration (if using RDS)
# RDS_USERNAME=admin
# RDS_PASSWORD=your_password_here
# RDS_DATABASE=semantic_search
